{
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "apps/vite-app/src",
  "targets": {
    "build": {
      "executor": "@nx-plus/vite:build",
      "options": {
        "config": "apps/vite-app/vite.config.ts"
      }
    },
    "serve": {
      "executor": "@nx-plus/vite:server",
      "options": {
        "config": "apps/vite-app/vite.config.ts"
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/vite-app/**/*.{ts,tsx,vue}"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/apps/vite-app"],
      "options": {
        "jestConfig": "apps/vite-app/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
